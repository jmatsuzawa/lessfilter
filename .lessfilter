#!/bin/sh

case "$1" in
*.mo)
  if which msgunfmt >/dev/null 2>&1; then
    msgunfmt "$1"
  fi
  ;;
# In the case of the other formats,
# return an error to pass it to lesspipe.sh provided by Red Hat
*)
  if file "$1" | grep ' data$' >/dev/null 2>&1; then
    hexdump -Cv "$1"
  else
    exit 1
  fi
  ;;
esac
